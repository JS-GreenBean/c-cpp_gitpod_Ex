#include <stdio.h>

int main()
{
    int i = 1;
    
    printf("please input a number \n");
    scanf("%d",&i);
    
    switch(i){
        case 1:
        printf("this is number 1 \n");
        break
        case 2;
        printf("this is number 2 \n");
        default;
        printf("please input a number 1  or 2")
    }
    
    cout<<"Hello World";
    
    return 0;
